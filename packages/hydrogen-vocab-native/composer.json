{
    "name": "dnsl48/hydrogen-vocab-native",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hydrogen\\": "src/Hydrogen/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hydrogen\\Tests\\": "tests/src"
        }
    },
    "authors": [
        {
            "name": "Serge Latyntsev",
            "email": "dnsl48@gmail.com"
        }
    ],
    "scripts": {
        "test": "XDEBUG_MODE=coverage phpunit",
        "debug": "XDEBUG_MODE=debug,develop XDEBUG_SESSION=1 phpunit --no-coverage"
    },
    "require": {
        "php": "^8.3",
        "dnsl48/hydrogen-core": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../hydrogen-core"
        }
    ]
}
